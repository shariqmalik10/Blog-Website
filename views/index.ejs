<div class="main-section">
    <h1>Hello User X!</h1>
    <div class="blog-list">
        <% if (locals.title) { %>
            <h1>Blogs written by User X</h1><br>
            <div class="blog">
                <h1>Title</h1>
                <h3><%= locals.title %></h3>
                
                <h1>Content</h1>
                <p><%= locals.content %></p>
            </div>
        <% } else {%>
            <h1>No blogs to display</h1>
        <% } %>
    </div>

    
    
    
</div>

